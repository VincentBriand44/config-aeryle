{
  "name": "subbedstar",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "env:dev": "cross-env NODE_ENV=development pnpm run",
    "env:prod": "cross-env NODE_ENV=production pnpm run",
    "dev": "vite dev --host",
    "_dev:android": "pnpm cap:sync && pnpm dev",
    "dev:android": "pnpm env:dev _dev:android",
    "build": "vite build",
    "_build:android": "pnpm build && pnpm cap:build  android",
    "build:android": "pnpm env:prod _build:android",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "up": "pnpx taze -wl",
    "up:major": "pnpm run up major",
    "cap": "capacitor",
    "cap:sync": "cap sync",
    "cap:build": "pnpm cap:sync && cap build"
  },
  "devDependencies": {
    "@capacitor/cli": "6.0.0",
    "@capacitor/preferences": "6.0.0",
    "@eslint/eslintrc": "3.0.2",
    "@sapphire/shapeshift": "3.9.7",
    "@skeletonlabs/skeleton": "2.10.0",
    "@skeletonlabs/tw-plugin": "0.4.0",
    "@sveltejs/adapter-static": "3.0.1",
    "@sveltejs/kit": "2.5.7",
    "@sveltejs/vite-plugin-svelte": "4.0.0-next.0",
    "@types/eslint": "8.56.10",
    "@types/node": "20.12.11",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "autoprefixer": "10.4.19",
    "cross-env": "7.0.3",
    "cz-emoji": "1.3.2-canary.2",
    "dotenv": "16.4.5",
    "eslint": "9.2.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-svelte": "2.39.0",
    "globals": "15.2.0",
    "houdini": "^1.2.47",
    "houdini-svelte": "^1.2.47",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-svelte": "3.2.3",
    "prettier-plugin-tailwindcss": "0.5.14",
    "svelte": "5.0.0-next.130",
    "svelte-check": "3.7.1",
    "svelte-eslint-parser": "0.36.0",
    "tailwindcss": "3.4.3",
    "tslib": "2.6.2",
    "typescript": "5.4.5",
    "typescript-eslint": "7.8.0",
    "vite": "5.2.11"
  },
  "trustedDependencies": [
    "@sveltejs/kit",
    "esbuild",
    "svelte-preprocess"
  ],
  "dependencies": {
    "@badgateway/oauth2-client": "2.3.0",
    "@capacitor/android": "6.0.0",
    "@capacitor/app": "6.0.0",
    "@capacitor/core": "6.0.0",
    "@capacitor/ios": "6.0.0",
    "svelte-heros-v2": "2.0.0-next.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    },
    "cz-emoji": {
      "skipQuestions": [
        "scope",
        "body",
        "breaking",
        "issues"
      ]
    }
  },
  "packageManager": "pnpm@9.1.0"
}
