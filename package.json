{
  "name": "config-aeryle",
  "version": "1.0.1",
  "description": "Default configs for ESLint, Prettier",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "format": "eslint . --fix"
  },
  "keywords": [
    "eslint",
    "prettier",
    "config"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/eslintrc": "3.0.2",
    "@eslint/js": "9.2.0",
    "@types/node": "20.12.12",
    "cz-emoji": "1.3.2-canary.2",
    "eslint": "9.2.0",
    "prettier": "3.2.5",
    "prettier-plugin-svelte": "3.2.3",
    "prettier-plugin-tailwindcss": "0.5.14",
    "svelte-eslint-parser": "0.36.0",
    "typescript-eslint": "7.9.0"
  },
  "peerDependencies": {
    "eslint": "^9",
    "prettier": "^3",
    "eslint-plugin-svelte": "^2",
    "eslint-plugin-prettier": "^5",
    "eslint-config-prettier": "^9",
    "eslint-plugin-import": "^2"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-prettier": {
      "optional": true
    },
    "eslint-config-prettier": {
      "optional": true
    },
    "eslint-plugin-import": {
      "optional": true
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    },
    "cz-emoji": {
      "skipQuestions": [
        "scope",
        "body",
        "breaking",
        "issues"
      ]
    }
  }
}
